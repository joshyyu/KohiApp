<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="@color/color_dark_reddish_brown"/>
            <padding
                android:bottom="30dp"
                android:left="30dp"
                android:right="30dp"
                android:top="30dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/light_brown"/>
        </shape>
    </item>
    <item android:gravity="top|left"
        android:left="-24dp"
        android:top="-23dp">
        <shape
            android:shape="oval">
            <solid
                android:color="#FFFFFF"/>
            <size
                android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>
    <item android:gravity="bottom|right"
        android:right="-22dp"
        android:bottom="-21dp">
        <shape
            android:shape="oval">
            <solid
                android:color="#FFFFFF"/>
            <size
                android:width="8dp"
                android:height="8dp"/>
        </shape>
    </item>
</layer-list>
